{
  "name": "bike-park-finder-api",
  "version": "1.0.0",
  "description": "GraphQL API for Bike Park Finder application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "node dist/index.js",
    "start": "nodemon",
    "debug": "nodemon --inspect src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node --loader ts-node/esm src/scripts/seed.ts",
    "seed:build": "npm run build && node dist/scripts/seed.js",
    "seed:mock": "node src/scripts/generateMockDb.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "express-server": "node --loader ts-node/esm src/express-server.js"
  },
  "keywords": [
    "graphql",
    "api",
    "bike-park"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.2",
    "@types/express": "^5.0.1",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "express-graphql": "^0.12.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.1",
    "graphql": "^16.6.0",
    "graphql-upload": "^17.0.0",
    "graphql-yoga": "^5.13.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.30.1",
