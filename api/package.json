    "@types/bcryptjs": "^2.4.2",
    "@types/express-session": "^1",
    "@types/graphql-upload": "^17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6",
    "faker": "^6.6.6",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
